//First task

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    for (int i = 5; i >= 1; i--){
    	System.out.println(i);
	  }
  }
}

//Second task

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int n = in.nextInt();
    int a = 0;
    for (int i = 0; i < n; i++){
    	a += i + 1;
	  }
    System.out.println(a);
    in.close();
  }
}

